<!--pages/name/name.wxml-->
<view class='body'>
<form bindsubmit='formSubmit' >
  <view class='tar1'>
            <view class="weui-cells weui-cells_after-title">
            <view  class="weui-cell weui-cell_access" >
                <view class="weui-cell__bd">学校</view>
                <input class="weui-cell__ft weui-cell__ft_in-access" value='{{schoolname}}' name='schoolname'></input>
            </view>
        </view>
        <view class="weui-cells weui-cells_after-title">
            <view  class="weui-cell weui-cell_access" >
                <view class="weui-cell__bd">学历</view>
                <input class="weui-cell__ft weui-cell__ft_in-access"  value='{{degree}}' name='degree'></input>
            </view>
        </view>
        <view class="weui-cells weui-cells_after-title">
            <view  class="weui-cell weui-cell_access" >
                <view class="weui-cell__bd">专业</view>
                <input class="weui-cell__ft weui-cell__ft_in-access" value='{{major}}' name='major'></input>
            </view>
        </view>
  </view>  
  <view class='tar2'>
  <button formType='submit' style='background-color:#139AFF' ><text style='color:white'>保存</text></button>
  </view>    
   </form>                 
</view>
